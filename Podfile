# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'AutoChessHelper' do
# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!

    # Pods for AutoChessHelper
    pod 'SwiftyGif'
    pod 'RealmSwift'
    pod 'RxSwift',    '~> 4.0'
    pod 'RxCocoa',    '~> 4.0'

target 'AutoChessHelperTests' do
    use_frameworks!
  
    # Pods for testing
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
inherit! :search_paths
# Pods for testing
end

target 'AutoChessHelperUITests' do
inherit! :search_paths
# Pods for testing
end

end
